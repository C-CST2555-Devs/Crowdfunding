mkdir -p Controllers Models Data crowdfunding-frontend 
dotnet ef migrations add InitialCreate
dotnet ef database update 
dotnet add package Microsoft.EntityFrameworkCore.SqlServer
dotnet add package Microsoft.EntityFrameworkCore.Design
dotnet tool install --global dotnet-ef 
dotnet run 
cd crowdfunding-frontend
npx http-server 

# Initialize git repository
git init

# Add all files
git add .

# Create initial commit
git commit -m "Initial commit: Setup crowdfunding project structure"

# Add the remote repository
git remote add origin https://github.com/C-CST2555-Devs/Crowdfunding.git

# Push to main branch
git push -u origin main 